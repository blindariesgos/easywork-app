{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", ".next/types/**/*.ts", "**/*.ts", "**/*.tsx", "src/app/[locale]/(home)/agents-management/capacitations/e-learning/courses/services/get-courses.js", "src/hooks/use-boolean.js", "src/sections/calendar/hooks/index.js", "src/sections/calendar/calendar-tabs.jsx", "src/constants/index.js", "src/hooks/use-set-state.js"],
  "exclude": ["node_modules"]
}
